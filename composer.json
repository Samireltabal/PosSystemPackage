{
    "name": "synciteg/pos-system",
    "description": "Pos System created for syncit project",
    "type": "Package",
    "keywords" : ["Point Of Sale", "Syncit", "Products", "invoices", "selling"],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.65",
        "laravel/passport": "^10.1",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-permission": "^5.3",
        "milon/barcode": "^8.0",
        "spatie/laravel-tags": "^3.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Samir M. Eltabal",
            "email": "samir@synciteg.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Synciteg\\PosSystem\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Synciteg\\PosSystem\\PosSystemServiceProvider"
            ],
            "aliases": {
                "PosSystem": "Synciteg\\PosSystem\\PosSystemFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
